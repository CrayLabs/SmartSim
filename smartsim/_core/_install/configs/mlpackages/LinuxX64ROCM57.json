{
    "platform": {
        "operating_system":"linux",
        "architecture":"x86_64",
        "device":"rocm-5"
    },
    "ml_packages": [
        {
            "name": "dlpack",
            "version": "v0.5_RAI",
            "pip_index": "",
            "python_packages": [],
            "lib_source": "https://github.com/RedisAI/dlpack.git"
        },
        {
            "name": "libtorch",
            "version": "2.2.2",
            "pip_index": "https://download.pytorch.org/whl/rocm5.7",
            "python_packages": [
                "torch==2.2.2+rocm5.7",
                "torchvision==0.17.2+rocm5.7",
                "torchaudio==2.2.2+rocm5.7"
            ],
            "lib_source": "https://github.com/CrayLabs/ml_lib_builder/releases/download/v0.2/libtorch-2.2.2-linux-x64-rocm-5.7.0.tgz",
            "rai_patches": [
                {
                    "description": "Patch RedisAI module to require C++17 standard instead of C++14",
                    "source_file": "src/backends/libtorch_c/CMakeLists.txt",
                    "regex": "set_property\\(TARGET\\storch_c\\sPROPERTY\\sCXX_STANDARD\\s(98|11|14)\\)",
                    "replacement": "set_property(TARGET torch_c PROPERTY CXX_STANDARD 17)"
                }
            ]
        },
        {
            "name": "onnxruntime",
            "version": "1.17.3",
            "pip_index": "",
            "python_packages": [
                "onnx==1.15",
                "skl2onnx",
                "scikit-learn",
                "onnxmltools"
            ],
            "lib_source": "https://github.com/CrayLabs/ml_lib_builder/releases/download/v0.2/onnxruntime-1.17.3-linux-x64-rocm-5.7.0.tgz"
        }
    ]
}
